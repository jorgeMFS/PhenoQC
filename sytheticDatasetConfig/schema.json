{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "PhenotypicData",
  "type": "object",
  "required": [
    "SampleID",
    "PrimaryPhenotype",
    "DiseaseCode"
  ],
  "properties": {
    "SampleID": {
      "type": ["string", "number", "null"]
    },
    "Height_cm": {
      "type": ["number", "null"]
    },
    "Weight_kg": {
      "type": ["number", "null"]
    },
    "Cholesterol_mgdl": {
      "type": ["number", "null"]
    },
    "BP_systolic": {
      "type": ["number", "null"]
    },
    "BP_diastolic": {
      "type": ["number", "null"]
    },
    "Glucose_mgdl": {
      "type": ["number", "null"]
    },
    "Creatinine_mgdl": {
      "type": ["number", "null"]
    },

    "PrimaryPhenotype": {
      "type": ["string", "null"]
    },
    "SecondaryPhenotype": {
      "type": ["string", "null"]
    },
    "DiseaseCode": {
      "type": ["string", "null"]
    },

    "ObservedFeatures": {
      "type": ["array", "string", "null"]
    },
    "VisitDate": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "SampleCollectionDateTime": {
      "type": ["string", "null"],
      "format": "date-time"
    },

    "GenomeSampleID": {
      "type": ["string", "null"]
    },
    "HospitalID": {
      "type": ["string", "null"]
    }
  }
}
